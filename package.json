{
  "name": "ts-graphql-yoga-example",
  "version": "0.0.1",
  "description": "Graphql Yoga Typescript Example",
  "main": "src/server.ts",
  "repository": "https://github.com/dooboolab/ts-graphql-yoga-example",
  "author": "dooboolab",
  "license": "MIT",
  "scripts": {
    "start": "NODE_ENV=development nodemon --exec ts-node src/server.ts",
    "watch": "nodemon --watch 'src/**/*.ts' --ignore 'src/**/*.spec.ts' --exec 'ts-node' src/index.ts",
    "lint": "eslint src/**/**/*.ts && eslint tests/**/**/*.ts",
    "tsc": "tsc -skipLibCheck",
    "test": "NODE_ENV=test jest",
    "test:coverage": "jest -w 1 --coverage",
    "test:watch": "jest --watch",
    "codegen": "graphql-codegen",
    "generate": "graphql-codegen --config codegen.yml",
    "codecov": "codecov"
  },
  "dependencies": {
    "graphql": "^14.4.2",
    "graphql-yoga": "^1.18.0",
    "jest": "^24.8.0",
    "jsonwebtoken": "^8.5.1",
    "merge-graphql-schemas": "^1.5.8",
    "mysql2": "^1.6.5",
    "sequelize": "^5.9.2"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^1.3.1",
    "@graphql-codegen/typescript": "^1.3.1",
    "@graphql-codegen/typescript-resolvers": "1.3.1",
    "@types/bluebird": "^3.5.27",
    "@types/jest": "^24.0.15",
    "@types/jsonwebtoken": "^8.3.2",
    "@types/node": "^12.0.12",
    "@types/sequelize": "^4.28.3",
    "@types/validator": "^10.11.1",
    "@typescript-eslint/eslint-plugin": "^1.11.0",
    "@typescript-eslint/parser": "^1.11.0",
    "codecov": "^3.5.0",
    "dotenv": "^8.0.0",
    "eslint": "^6.0.1",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-node": "^9.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.0",
    "nodemon": "^1.19.1",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.3.0",
    "typescript": "^3.5.2"
  },
  "resolutions": {
    "graphql": "14.4.2"
  }
}
