{
  "name": "graphql-yoga-ts-example",
  "version": "0.0.1",
  "description": "Graphql Yoga Typescript Example",
  "main": "src/server.ts",
  "repository": "https://github.com/dooboolab/graphql-yoga-ts-example",
  "author": "dooboolab",
  "license": "MIT",
  "scripts": {
    "start": "node node_modules/ts-node/dist/bin.js ./src/server.ts",
    "watch": "nodemon --watch 'src/**/*.ts' --ignore 'src/**/*.spec.ts' --exec 'ts-node' src/index.ts",
    "lint": "eslint **/*.ts",
    "tsc": "tsc",
    "test": "jest -w 1",
    "test:coverage": "jest -w 1 --coverage",
    "test:watch": "jest --watch",
    "codegen": "graphql-codegen",
    "generate": "graphql-codegen --config codegen.yml"
  },
  "dependencies": {
    "graphql": "^14.2.1",
    "graphql-yoga": "^1.14.10",
    "jest": "^24.7.1",
    "merge-graphql-schemas": "^1.5.8",
    "mysql2": "^1.6.5",
    "sequelize": "^4.38.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^1.1.1",
    "@graphql-codegen/typescript": "^1.1.1",
    "@graphql-codegen/typescript-resolvers": "1.1.1",
    "@types/jest": "^24.0.11",
    "@types/node": "^11.13.6",
    "@types/sequelize": "^4.27.48",
    "@types/supertest": "^2.0.7",
    "@typescript-eslint/eslint-plugin": "^1.7.0",
    "@typescript-eslint/parser": "^1.7.0",
    "eslint": "^5.16.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-promise": "^4.1.1",
    "eslint-plugin-standard": "^4.0.0",
    "eslint-plugin-typescript": "^0.14.0",
    "nodemon": "^1.18.11",
    "supertest": "^4.0.2",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.1.0",
    "typescript": "^3.4.5",
    "typescript-eslint-parser": "^22.0.0"
  }
}
